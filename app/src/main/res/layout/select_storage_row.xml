<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="35dp"
        android:id="@+id/imageView4"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Organisation name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:id="@+id/orgname_tv_sto" />

    <TextView
        android:text="Org ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/orgname_tv_sto"
        android:layout_toRightOf="@+id/orgname_tv_sto"
        android:layout_toEndOf="@+id/orgname_tv_sto"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:id="@+id/orgID_tv_sto" />

    <TextView
        android:text="ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/orgID_tv_sto"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/id_view_sto" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nonPerPrice_et_sto"
        android:layout_below="@+id/perPrice_et_sto"
        android:layout_alignLeft="@+id/perPrice_et_sto"
        android:layout_alignStart="@+id/perPrice_et_sto"
        android:layout_marginTop="12dp"
        android:text="Non-perishable"/>

    <TextView
        android:text="Max W"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:id="@+id/maxW_et_sto"
        android:layout_below="@+id/nonPerPrice_et_sto"
        android:layout_alignLeft="@+id/nonPerPrice_et_sto"
        android:layout_alignStart="@+id/nonPerPrice_et_sto" />

    <TextView
        android:text="Min W"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/minW_et_sto"
        android:layout_below="@+id/maxW_et_sto"
        android:layout_alignLeft="@+id/maxW_et_sto"
        android:layout_alignStart="@+id/maxW_et_sto"
        android:layout_marginTop="12dp" />

    <TextView
        android:text="Min_Sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/minS_et_sto"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/maxS_et_sto"
        android:layout_alignLeft="@+id/maxS_et_sto"
        android:layout_alignStart="@+id/maxS_et_sto" />

    <TextView
        android:text="TP pr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tp_tv_sto"
        android:layout_below="@+id/minS_et_sto"
        android:layout_alignLeft="@+id/minS_et_sto"
        android:layout_alignStart="@+id/minS_et_sto"
        android:layout_marginTop="11dp" />

    <TextView
        android:text="Perishable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/perPrice_et_sto"
        android:layout_below="@+id/imageView4"
        android:layout_alignLeft="@+id/orgname_tv_sto"
        android:layout_alignStart="@+id/orgname_tv_sto" />

    <TextView
        android:text="Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tp_tv_sto"
        android:layout_alignRight="@+id/maxS_et_sto"
        android:layout_alignEnd="@+id/maxS_et_sto"
        android:layout_marginTop="34dp"
        android:id="@+id/orgLoc_tv_sto" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/orgLoc_tv_sto"
        android:layout_alignLeft="@+id/orgLoc_tv_sto"
        android:layout_alignStart="@+id/orgLoc_tv_sto"
        android:layout_marginTop="11dp"
        android:id="@+id/orgCont_tv_sto"
        android:text="Contact no" />

    <TextView
        android:text="Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:id="@+id/orgEmail_tv_sto"
        android:layout_below="@+id/orgCont_tv_sto"
        android:layout_alignLeft="@+id/orgCont_tv_sto"
        android:layout_alignStart="@+id/orgCont_tv_sto" />

    <TextView
        android:text="Username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/orgEmail_tv_sto"
        android:layout_alignLeft="@+id/orgEmail_tv_sto"
        android:layout_alignStart="@+id/orgEmail_tv_sto"
        android:layout_marginTop="16dp"
        android:id="@+id/orgusern_tv_sto" />

    <TextView
        android:text="Non-perishable/Kg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_alignBaseline="@+id/nonPerPrice_et_sto"
        android:layout_alignBottom="@+id/nonPerPrice_et_sto"
        android:layout_alignLeft="@+id/perP"
        android:layout_alignStart="@+id/perP" />

    <TextView
        android:text="Min Weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minW_et_sto"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignEnd="@+id/textView8"
        android:id="@+id/textView9" />

    <TextView
        android:text="Max Weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_alignBaseline="@+id/maxW_et_sto"
        android:layout_alignBottom="@+id/maxW_et_sto"
        android:layout_toRightOf="@+id/perPrice_et_sto"
        android:layout_toEndOf="@+id/perPrice_et_sto"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp" />

    <TextView
        android:text="Max Space"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/maxS_et_sto"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:id="@+id/textView10" />

    <TextView
        android:text="Min Space"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/minS_et_sto"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:id="@+id/textView11" />

    <TextView
        android:text="Transport price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:layout_above="@+id/orgLoc_tv_sto"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:text="Perishable/Kg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/perP"
        android:layout_above="@+id/nonPerPrice_et_sto"
        android:layout_toRightOf="@+id/orgname_tv_sto"
        android:layout_toEndOf="@+id/orgname_tv_sto" />

    <TextView
        android:text="Max_SP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/maxS_et_sto"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/minW_et_sto"
        android:layout_alignLeft="@+id/minW_et_sto"
        android:layout_alignStart="@+id/minW_et_sto" />

</RelativeLayout>